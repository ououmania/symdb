<Config>
    <Listen>/tmp/symdb.sock</Listen>
    <DataDir>/home/richy/.symdb/data</DataDir>
    <LogDir>/home/richy/.symdb/log</LogDir>

    <!-- If not set, get from "g++ -E -x c++ - -v < /dev/null 2>&1" -->
    <SystemInclude>
        <Directory>/usr/local/include/</Directory>
        <Directory>/usr/include/c++/8.3.0</Directory>
        <Directory>/usr/include/c++/8.3.0/x86_64-pc-linux-gnu</Directory>
        <Directory>/usr/include/</Directory>
        <Directory>/usr/lib/gcc/x86_64-pc-linux-gnu/8.3.0/include-fixed</Directory>
        <Directory>/usr/lib/gcc/x86_64-pc-linux-gnu/8.3.0/include/</Directory>
    </SystemInclude>

    <GlobalExcluded>
        <!--Exclude all protobuf generated files -->
        <ExcludeEntry pattern=".*\\.pb\\.(cc|h)$" />
        <ExcludeEntry pattern=".*/_?build/.*" />
    </GlobalExcluded>
    <Projects>
        <Project>
            <Name>symdb</Name>
            <Home>/home/richy/git/symdb</Home>
        </Project>
    </Projects>
</Config>
